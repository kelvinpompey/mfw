ul, li, body, img {
    margin: 0;
    padding: 0;
}

ul li {
    padding: 20px;
}

body {
    width: 1280px;
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
  background-color: #383838;;
  color: #ffffff;
    margin: auto;

}

a {
  color: #00B7FF;
}

.horizontal {
    height: 50px;
}

.horizontal li{
    display: inline;
    list-style-type: none;
}

#topnav {
    float: right;
    padding: 0px;
    padding-top: 30px
}

#sidebar {
    clear: both;
    float: left;
    width: 170px;
    background-color: #383838;
}

#sidebar ul li {
    list-style-type: none;
    border-bottom: solid 1px silver;
    padding: 20px;
    text-align: center;
}

#sidebar ul li#facebook {
    border-bottom: none 1px silver;
}

#sidebar ul li#twitter {
    border-bottom: none 1px silver;
}

#logo {
    float: left;
    background-image: url("../images/mfw-logo.png");
    background-repeat: no-repeat;
    height: 70px;
    width: 200px;
}

header {
    height: 80px;
    background-image: url('../images/rastaborder.png');
    background-repeat: no-repeat;
    background-position: bottom;
    /*

    -moz-border-image: url('../images/rastaborder.png');
    -webkit-border-image: url('../images/rastaborder.png');
    border-image: url('../images/rastaborder.png');
    */


}

#coverimage
    background-color: #C7CAD3;
    height: 600px;
    img#rascollection
        position: relative;
        bottom: 150px;
        padding: 80px;
        padding-bottom: 0

#main {
    float: left;
    background-color: #383838;
}

#imageslider {
    background-color: #323232;
    position: relative;
    height: 425px;
    width: 1108px;
}

#previous {
    position: absolute;
    top: 150px;
    left: 0;
    background-color: #919397;

    padding: 5px;
    padding-top: 20px;
    padding-bottom: 20px;
}

#next {
    background-color: #919397;
    position: absolute;
    top: 150px;
    right: 0;
    padding: 5px;
    padding-top: 20px;
    padding-bottom: 20px;
}
#imageslider img {
    float: left;
    border-top: solid 5px black;
    border-bottom: solid 5px black;
}